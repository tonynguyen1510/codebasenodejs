{
	"name": "StudySession",
	"plural": "study-sessions",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"scope": {
		"order": "createdAt DESC"
	},
	"properties": {
		"createdAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"updatedAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"startDate": {
			"type": "date",
			"defaultFn": "now"
		},
		"endDate": {
			"type": "date"
		},
		"content": {
			"type": "string"
		},
		"note": {
			"type": "string"
		}
	},
	"validations": [],
	"relations": {
		"creator": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "creatorId"
		},
		"lesson": {
			"type": "belongsTo",
			"model": "Lesson",
			"foreignKey": "lessonId"
		},
		"details": {
			"type": "hasMany",
			"model": "StudySessionDetail",
			"foreignKey": "sessionId"
		}
	},
	"acls": [],
	"methods": {}
}

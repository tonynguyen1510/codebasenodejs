{
	"name": "StudySession",
	"plural": "study-sessions",
	"base": "PersistedModel",
	"idInjection": true,
	"options": {
		"validateUpsert": true
	},
	"scope": {
		"order": "createdAt DESC"
	},
	"properties": {
		"createdAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"updatedAt": {
			"type": "date",
			"defaultFn": "now"
		},
		"startTime": {
			"type": "date"
		},
		"endTime": {
			"type": "date"
		},
		"status": {
			"type": "string",
			"default": "live"
		},
		"lessonData": {
			"type": "object"
		},
		"isDelete": {
			"type": "boolean",
			"default": false
		},
		"note": {
			"type": "string"
		}
	},
	"validations": [],
	"relations": {
		"creator": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "creatorId"
		},
		"lesson": {
			"type": "belongsTo",
			"model": "Lesson",
			"foreignKey": "lessonId"
		},
		"level": {
			"type": "belongsTo",
			"model": "Level",
			"foreignKey": "levelName",
			"primaryKey": "name"
		},
		"details": {
			"type": "hasMany",
			"model": "StudySessionDetail",
			"foreignKey": "sessionId"
		}
	},
	"acls": [],
	"methods": {}
}
